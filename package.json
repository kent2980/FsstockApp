    "react-native-web": "~0.18.10",
    "styled-components": "^5.3.10"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@types/styled-components": "^5.1.26",
    "babel-plugin-macros": "^3.1.0",
    "babel-plugin-transform-inline-environment-variables": "^0.4.4",
